      <Button fx:id="playGame" layoutX="124.0" layoutY="327.0" mnemonicParsing="false" onAction="#onPlayGame" prefHeight="26.0" prefWidth="80.0" text="Play Game" />
      <TextField fx:id="playerName" layoutX="225.0" layoutY="208.0" promptText="Name" />
